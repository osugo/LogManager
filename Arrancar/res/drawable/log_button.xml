<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape>
            <solid 
                android:color="@color/CalmGreen"/>
            <stroke 
                android:width="1dip"
                android:color="@color/CalmGreen"/>
            <padding 
                android:left="10dip"
                android:top="10dip"
                android:right="10dp"
                android:bottom="10dip"/>
        </shape>  
    </item>
	<item>
	    <shape>
	        <solid
	            android:color="@color/MellowGreen"
	            />
	        <stroke 
	            android:width="1dip"
	            android:color="@color/MellowGreen"/>
	        
	        <padding
	             android:left="10dip"
                android:top="10dip"
                android:right="10dp"
                android:bottom="10dip"  />
	    </shape>
	</item>
</selector>
